<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
    <metadata minClientVersion="0.7.2">
        <id>CatBrows</id>
        <version>0.7.2</version>
        <title>CatBrows</title>
        <authors>Sebastian Hallén</authors>
        <projectUrl>https://github.com/sebastianhallen/CatBrows</projectUrl>
        <licenseUrl>https://raw.github.com/sebastianhallen/CatBrows/master/LICENSE</licenseUrl>
        <dependencies>
          <dependency id="SpecFlow" />
          <dependency id="NUnit" />
        </dependencies>
        <frameworkAssemblies>System.Configuration</frameworkAssemblies>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>CatBrows is a SpecFlow generator plugin that adds the possibility to tag you scenarios with @Browser:SomeBrowser tags. Each @Browser-tag will result in a separate test run where value for the browser is stored in ScenarioContext.Current["Browser"].</description>
        <summary>Browser tagging for spec flow scenarios.</summary>
        <tags>specflow nunit browser test</tags>
    </metadata>
    <files>
        <file src="..\CatBrows.Generator\bin\Release\CatBrows.Generator.SpecFlowPlugin.dll" target="tools\CatBrows.Generator.SpecFlowPlugin.dll" />
        <file src="App.config.install.xdt" target="content" />
        <file src="App.config.uninstall.xdt" target="content" />
    </files>
</package>
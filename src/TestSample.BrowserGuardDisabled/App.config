<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow" />
  </configSections>

  <specFlow>

    <plugins>
      <add name="CatBrows" path="..\NuGetPackage\plugin" type="Generator" />
    </plugins>
    <stepAssemblies>
      <stepAssembly assembly="TestSample" />
    </stepAssemblies>
    <generator allowDebugGeneratedFiles="true" />
    <runtime missingOrPendingStepsOutcome="Error" />
    <unitTestProvider name="NUnit" />
  </specFlow>
  <appSettings>
    <add key="CatBrows-RequiresBrowser" value="false" />
  </appSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>
